{
  "name": "@whoj/manage-pkg",
  "version": "0.1.1-beta.1",
  "description": "Install & Uninstall package programmatically.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "author": {
    "name": "Jonson B.",
    "email": "www.jbc.bd@gmail.com"
  },
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/who-jonson/manage-pkg/issues"
  },
  "homepage": "https://github.com/who-jonson/manage-pkg#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/manage-pkg.git"
  },
  "keywords": [
    "npm",
    "pnpm",
    "yarn",
    "universal"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "dev": "pnpm build --watch",
    "start": "esno src/index.ts",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "lint:fix": "pnpm lint --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@antfu/ni": "^0.16.2",
    "execa": "^5.1.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
